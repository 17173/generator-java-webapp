<table class="table table-striped table-hover">
<thead>
  <tr>
    <th>组件名称</th>
    <th>创建者</th>
    <th>创建时间</th>
    <th>操作</th>
  </tr>
</thead>
<tbody data-role="rows">
  {{#each listData}}
  <tr>
    <td>{{componentName}}</td>
    <td>{{createUserName}}</td>
    <td>{{fdate createTime}}</td>
    <td>
      <a href="javascript:;" data-index="{{@index}}" data-role="remove">删除</a>
    </td>
  </tr>
  {{/each}}
</tbody>
</table>