<form class="form-horizontal ue-form">
  {{#each views}}
  <div class="form-group">
    <label class="col-sm-2 control-label">{{label}}</label>
    <div class="col-sm-10">
      <p class="form-control-static" style="{{css}}" data-name="{{name}}">
        {{{value}}}
      </p>
    </div>
  </div>
  {{/each}}
</form>
