{
  "name": "gallery",
  "version": "1.0.0",
  "description": "a javascript gallery plugin",
  "keywords": [
    "gallery",
    "pandora",
    "widget",
    "seajs"
  ],
  "homepage": "https://github.com/pandorajs/gallery",
  "bugs": {
    "url": "https://github.com/pandorajs/gallery/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/pandorajs/gallery/blob/master/LICENSE-MIT"
    }
  ],
  "author": {
    "name": "pandorajs",
    "url": "https://github.com/pandorajs"
  },
  "main": "Gruntfile.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/pandorajs/gallery.git"
  },
  "scripts": {
    "test": "grunt qunit"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "dependencies": {},
  "devDependencies": {
    "grunt": "*",
    "grunt-cmd-transport": "crossjs/grunt-cmd-transport",
    "grunt-cmd-concat": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-jshint": "*",
    "grunt-contrib-uglify": "*",
    "grunt-contrib-yuidoc": "*",
    "grunt-coveralls": "*",
    "grunt-qunit-istanbul": "*",
    "matchdep": "*"
  },
  "family": "pandora",
  "spm": {
    "output": [
      "gallery.js","thumbnails.js","fullscreen.js"
    ],
    "alias": {
      "$": "$",
      "widget": "pandora/widget/1.0.0/widget",
      "overlay": "pandora/overlay/1.0.0/overlay",
      "locker": "pandora/locker/1.0.0/locker",
      "importstyle": "pandora/importstyle/1.0.0/importstyle",
      "handlebars-runtime": "gallery/handlebars/1.3.0/handlebars-runtime",
      "handlebars": "gallery/handlebars/1.3.0/handlebars"
    }
  }
}